    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/LINUX/bin/li",
            "args": ["-D", "/home/anfu/Desktop/mutation/printtokens/genmutant/print_tokens/", "-P", "__print_tokens", "print_tokens", "__print_tokens"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "sourceFileMap": {
                "/build/glibc-S9d2JN": "/usr/src/glibc"
            }
        }
    ]
